#include<stdio.h>
int main()
{
    int i,j;
    int arr[2]={10,20};
    int tem;
   printf("enter the value =");
   scanf("%d,%d,&arr[0],&arr[1]");
   for(i=0;i<=2;i++)
   {
       for(j=i;j<=i;j++)
       {
           tem=arr[0];
           arr[1]=arr[0];
           arr[1]=tem;
           printf(" after swapping\n");
       }
       printf("first number=%d\n",arr[0]);
       }
       printf("second number =%d\n",arr[1]);

    return 0;
}
