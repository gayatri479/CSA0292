#include <stdio.h>

int main() {
    int num, i;
    printf("Enter the num: ");
    scanf("%d", &num);

    printf("Even numbers from 1 to %d are:\n", num);
    for (i = 1; i <= num; i++) {
        if (i % 2 == 0) {
            printf("%d ", i);
        }
    }

    printf("\n");
    return 0;
output:
2,4,6,8,10
