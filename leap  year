#include<stdio.h>
int main()
{
    int year=2028;
    if(year%400==0){
    printf("year is leap year");
    }
    else if(year%100==0)
    {
    printf("year is not leap year");
   }
   else if(year%4==0)
   {
    printf("year is leap year");
   }
   else{
        printf("year is not leap year");
    }
    return 0;
    
}
output:
year is leap year
